<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="599.0" prefWidth="963.0" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="application.DataSubagianController">
   <children>
      <Text fill="#505bda" layoutX="48.0" layoutY="58.0" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-weight: 900;" text="Data Subagian">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <TableView fx:id="tabelSubagian" layoutX="48.0" layoutY="80.0" prefHeight="492.0" prefWidth="431.0">
        <columns>
          <TableColumn fx:id="no" prefWidth="75.0" text="no" />
            <TableColumn fx:id="nama" prefWidth="95.0" text="nama" />
            <TableColumn fx:id="warna" prefWidth="105.0" text="warna" />
            <TableColumn fx:id="action" prefWidth="155.0" text="action" />
        </columns>
      </TableView>
      <TitledPane animated="false" layoutX="559.0" layoutY="80.0" prefHeight="342.0" prefWidth="349.0" text="Tambah data baru">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="namaSubagian" layoutX="24.0" layoutY="42.0" prefHeight="40.0" prefWidth="300.0" promptText="Nama subagian" />
                  <ColorPicker fx:id="colorPicker" layoutX="24.0" layoutY="94.0" onAction="#pilihWarna" prefHeight="40.0" prefWidth="300.0" />
                  <Button fx:id="simpan" layoutX="172.0" layoutY="158.0" mnemonicParsing="false" onAction="#simpan" prefHeight="40.0" prefWidth="152.0" style="-fx-background-color: #505bda;" text="simpan data baru" textFill="WHITE" />
                  <Button fx:id="reset" layoutX="103.0" layoutY="158.0" mnemonicParsing="false" onAction="#resetBtn" prefHeight="42.0" prefWidth="63.0" style="-fx-background-color: #ececec;" text="Reset" />
               </children>
            </AnchorPane>
        </content>
      </TitledPane>
   </children>
</AnchorPane>
